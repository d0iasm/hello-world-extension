{
    "manifest_version": 2,

    "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "permissions": [
	"activeTab"
    ],
    "browser_action": {
	"default_title": "hello world",
	"default_icon": "icon.png"
    },
    "background": {
	"scripts": ["background.js"],
	"persistent": false
    }
}
